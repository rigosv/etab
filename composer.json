{
    "type": "project",
    "name": "minsal/etab",
    "license": "GPLv3",
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "friendsofsymfony/jsrouting-bundle": "^2.2",
        "friendsofsymfony/rest-bundle": "v2.3.1",
        "h4cc/wkhtmltopdf-amd64": "0.12.4",
        "knplabs/knp-snappy-bundle": "v1.5.2",
        "nelmio/api-doc-bundle": "^3.3",
        "onurb/excel-bundle": "^1.0",
        "php-amqplib/php-amqplib": "v2.7.2",
        "php-amqplib/rabbitmq-bundle": "^1.14.4",
        "predis/predis": "v1.1.1",
        "sensio/framework-extra-bundle": "^5.2",
        "snc/redis-bundle": "v2.1.5",
        "sonata-project/doctrine-orm-admin-bundle": "^3.6",
        "sonata-project/user-bundle": "^4.2",
        "symfony/acl-bundle": "^1.0",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/expression-language": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "^4.1",
        "symfony/security-bundle": "^4.1",
        "symfony/serializer-pack": "*",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-link": "^4.1",
        "symfony/webpack-encore-pack": "*",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "^1.0",
        "symfony/var-dumper": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "minimum-stability" : "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },    
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
